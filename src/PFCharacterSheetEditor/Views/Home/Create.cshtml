                            <option disabled selected hidden>Choose...</option>
                            <option>LG</option>
                            <option>NG</option>
                            <option>CG</option>
                            <option>LN</option>
                            <option>N</option>
                            <option>CN</option>
                            <option>LE</option>
                            <option>NE</option>
                            <option>CE</option>
                        </select>
                    </div>
                    <div class="form-group row">
                        <label for="PlayerName" class="col-form-label">Player Name OR Character Number<a href="http://paizo.com/pathfinderSociety/rpg" target="_blank" data-toggle="tooltip" data-placement="top" title="Character Number is only applicable if playing in Pathfinder Society">(?)</a></label>
                        <div class="col-sm-8">
                            <input type="text" class="form-control" id="PlayerName" placeholder="Your name, or PFS number." />
                        </div>
                    </div>
